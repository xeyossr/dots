/*
 If there is a certain color specially this #00ffff please report a bug 
 Theme: Wallbash
 Author: The HyDE Project
Description: A dynamic theme with a focus on readability and contrast.
 */

.theme-dark {
    --background-primary: #<wallbash_pry1>; /* bg for main workspace window  */
    --background-primary-alt: #<wallbash_1xa2>; /* code blocks bg for comands under tick marks */
    --background-secondary: #<wallbash_pry1>; /* bg for panels */
    --background-secondary-alt: #<wallbash_1xa1>;  /*when activewindow top border*/
    --background-accent: #00fff0;  /*?*/
    --background-modifier-border: #<wallbash_pry2>; /* border lines color for form fields */
    --background-modifier-form-field: #<wallbash_pry1>; /* bg for form fields eg search bar */ 
    --background-modifier-form-field-highlighted: #00fff0; /*?*/
    --background-modifier-box-shadow: rgba(0, 0, 0, 0.1);
    --background-modifier-success: #00fff0;
    --background-modifier-error: #<wallbash_pry2>; / * use for Clear button */
    --background-modifier-error-rgb: <wallbash_pry3_rgba>; /*?*/
    --background-modifier-error-hover: #00fff0; /*?*/  
    --background-modifier-cover: rgba(0, 0, 0, 0.8);
    --text-accent: #<wallbash_txt1>;
    --text-accent-hover: #<wallbash_txt2>;
    --text-normal: #<wallbash_pry4>; /* current active space*/
    --text-muted: #<wallbash_4xa8>; /* Dimmed not focused left list panel*/ 
    --text-faint: #<wallbash_4xa5>; /*tab titles*/
    --text-error: #<wallbash_pry3>;
    --text-error-hover: #<wallbash_pry4>;
    --text-highlight-bg: #<wallbash_1xa1>; /* ===hightlight==*/
    --text-selection: #<wallbash_1xa1>; /* selection highlight*/
    --text-on-accent: #<wallbash_txt4>; /* accent text */
     --interactive-normal: #<wallbash_1xa1>; /* Drop down menu bg*/
    --interactive-hover: #<wallbash_3xa9>;
    --interactive-accent-rgb: <wallbash_pry3_rgba>;
    --interactive-accent: #<wallbash_pry3>; 
}

.theme-light {
    --background-primary: #<wallbash_pry1>; /* bg for main workspace window  */
    --background-primary-alt: #<wallbash_1xa2>; /* code blocks bg for comands under tick marks */
    --background-secondary: #<wallbash_pry1>; /* bg for panels */
    --background-secondary-alt: #<wallbash_1xa1>;  /*when activewindow top border*/
    --background-accent: #00fff0;  /*?*/
    --background-modifier-border: #<wallbash_pry2>; /* border lines color for form fields */
    --background-modifier-form-field: #<wallbash_pry1>; /* bg for form fields eg search bar */ 
    --background-modifier-form-field-highlighted: #00fff0; /*?*/
    --background-modifier-box-shadow: rgba(0, 0, 0, 0.1);
    --background-modifier-success: #00fff0;
    --background-modifier-error: #<wallbash_pry2>; / * use for Clear button */
    --background-modifier-error-rgb: <wallbash_pry3_rgba>; /*?*/
    --background-modifier-error-hover: #00fff0; /*?*/  
    --background-modifier-cover: rgba(0, 0, 0, 0.8);
    --text-accent: #<wallbash_txt1>;
    --text-accent-hover: #<wallbash_txt2>;
    --text-normal: #<wallbash_pry4>; /* current active space*/
    --text-muted: #<wallbash_4xa8>; /* Dimmed not focused left list panel*/ 
    --text-faint: #<wallbash_4xa5>; /*tab titles*/
    --text-error: #<wallbash_pry3>;
    --text-error-hover: #<wallbash_pry4>;
    --text-highlight-bg: #<wallbash_1xa1>; /* ===hightlight==*/
    --text-selection: #<wallbash_1xa1>; /* selection highlight*/
    --text-on-accent: #<wallbash_txt4>; /* accent text */
     --interactive-normal: #<wallbash_1xa1>; /* Drop down menu bg*/
    --interactive-hover: #<wallbash_3xa9>;
    --interactive-accent-rgb: <wallbash_pry3_rgba>;
    --interactive-accent: #<wallbash_pry3>; 
}
